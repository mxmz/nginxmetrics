{
	"metrics": {
		"nginx_body_bytes_sent": {
			"value_source": "body_bytes_sent",
			"type":        "counter",
			"label_map": {
				"vhost":  "vhost",
				"method": "method"
			}
		},
		"nginx_request_time": {
			"value_source": "request_time",
			"type":        "summary",
			"label_map": {
				"vhost":  "vhost",
				"status": "status"
			}
		},
		"nginx_backend_response_time": {
			"value_source": "backend_response_time",
			"type":        "summary",
			"label_map": {
				"vhost":          "vhost",
				"backend_status": "backend_status"
			}
		},
		"nginx_error_count": {
			"value_source": "error",
			"type":        "counter",
			"label_map": {
			}
		},
		"nginx_crit_count": {
			"value_source": "error",
			"type":        "counter",
			"label_map": {
			}
		}
	}
}